<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="127.0" prefWidth="216.0" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.prueba2.HelloController">
    <VBox>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
      <children>
         <TextField />
          <Label fx:id="welcomeText" />
      </children>
    </VBox>
   <Button fx:id="boton1" mnemonicParsing="false" onAction="#prueba" text="Button" />
    <HBox alignment="CENTER" spacing="20.0">
        <Button onAction="#A" text="Boton A" />
        <Button onAction="#B" text="Boton B" />
        <Button onAction="#C" text="Boton C" />
    </HBox>
    <HBox alignment="CENTER" spacing="20.0">
        <Button onAction="#D" text="Boton D" />
        <Button onAction="#E" text="Boton E" />
        <Button onAction="#F" text="Boton F" />
    </HBox>
    <VBox>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </VBox>
</VBox>
